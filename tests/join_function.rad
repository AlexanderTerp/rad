a1 string[] = ["Hi", "there"]
print(join(a1, " "))
print(join(a1, " ", "Alice: "))
print(join(a1, " ", "Alice: ", "!"))

a2 int[] = [1, 2, 3]
print(join(a2, "_"))
print(join(a2, "_", "Nums: "))
print(join(a2, "_", "Nums: ", "_4"))

a3 float[] = [1.1, 1.2, 1.3]
print(join(a3, " yes "))
print(join(a3, " yes ", "Floats: "))
print(join(a3, " yes ", "Floats: ", " :D"))
